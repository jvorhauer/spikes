@startuml

class User {
  id: ULID
  name: String
  email: String
  born: LocalDate
  password: String
}

class Entry {
  id: ULID
}

class Task {
  title: String
  body: String
  due: LocalDateTime
  priority: Int
  status: TaskStatus
}
Entry <|-- Task

class Bookmark {
  title: String
  body: String
  url: String
}
Entry <|-- Bookmark

class WikiPage {
  title: String
  body: String
}

class Comment {
  body: String
}

User *-- Comment
User *-- Entry
Entry *-- Comment

@enduml
